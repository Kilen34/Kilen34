   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Key | Youtube Hub",
      Subtitle = "Key System",
      Note = "Key In Discord Server",
      FileName = "KhobyHandsome", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
